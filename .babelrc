{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "current",
          "uglify": true
        }
      }
    ],
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-decorators-legacy",
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ],
    "react-loadable/babel"
  ]
}
