{
  "name": "fe-template",
  "version": "1.0.0",
  "description": "Front end project template",
  "scripts": {
    "ssr": "node bin/server.js",
    "build-ssr": "rimraf ./dist && webpack --progress --config webpack/server/webpack.server.config.js",
    "dev": "webpack-dev-server --config webpack/client/webpack.client.config.js",
    "build": "rimraf ./dist && webpack --progress --config webpack/client/webpack.client.config.production.js",
    "loading": "rimraf ./dist/static/html/loading && webpack --config static/html/loading/webpack.loading.config.js",
    "login": "rimraf ./dist/static/html/login && webpack --config static/html/login/webpack.login.config.js",
    "eslint": "eslint \"src/**\" --fix"
  },
  "keywords": [
    "React",
    "React-Router-v4",
    "MobX",
    "Webpack",
    "ES6",
    "ES7"
  ],
  "author": "gongtiexin",
  "license": "ISC",
