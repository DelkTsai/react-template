{
  "name": "fe-template",
  "version": "1.0.0",
  "description": "Front end project template",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js",
    "build": "rimraf ./dist && webpack --progress --config webpack.config.production.js",
    "loading": "rimraf ./dist/static/html/loading && webpack --config static/html/loading/webpack.loading.config.js",
    "login": "rimraf ./dist/static/html/login && webpack --config static/html/login/webpack.login.config.js",
    "prettier-styles": "prettier --write \"src/**/*.less\"",
    "eslint": "eslint \"src/**\" --fix"
  },
  "keywords": [
    "React",
    "React-Router-v4",
    "MobX",
    "Webpack",
    "ES6",
    "ES7"
  ],
  "author": "gongtiexin",
  "license": "ISC",
