{
  "name": "react-template",
  "version": "1.0.0",
  "description": "frontend project template for react",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js",
    "build":
      "rimraf ./dist && webpack --progress --config webpack.config.production.js",
    "loading":
      "rimraf ./dist/static/html/loading && webpack --config static/html/loading/webpack.loading.config.js",
    "prettier":
      "prettier --trailing-comma es5  --write \"./**/*.js\" \"./**/*.less\" \"./**/*.json\" \"./**/*.md\"",
    "eslint": "eslint \"./**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier"
    }
  },
  "keywords": ["React", "React-Router-v4", "MobX", "Webpack", "ES6", "ES7"],
  "author": "gongtiexin",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-declaration-sorter": "^3.0.1",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.4",
    "image-webpack-loader": "^4.2.0",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "moment-locales-webpack-plugin": "^1.0.5",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.11.1",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.11.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "antd": "^3.5.2",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "echarts": "^4.1.0",
    "enquire.js": "^2.1.6",
    "lodash": "^4.17.5",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "mobx-react-devtools": "^5.0.1",
    "moment": "^2.22.0",
    "normalize.css": "^8.0.0",
    "number-precision": "^1.2.0",
    "particlesjs": "^2.2.2",
    "prop-types": "^15.6.1",
    "query-string": "^6.1.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.2.2",
    "rfx-core": "^1.6.0",
    "rxjs": "^6.1.0",
    "shortid": "^2.2.8",
    "up-utils": "3.6.0"
  }
}
